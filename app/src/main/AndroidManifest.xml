<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fop.anjal.footballourpassion">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LeagueActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TableActivity" />
        <activity android:name=".Teams.Team1" />
        <activity android:name=".Teams.Team2" />
        <activity android:name=".Teams.Team3" />
        <activity android:name=".Teams.Team4" />
        <activity android:name=".Teams.Team5" />
        <activity android:name=".Teams.Team6" />
        <activity android:name=".Teams.Team7" />
        <activity android:name=".Teams.Team8" />
        <activity android:name=".Teams.Team9" />
        <activity android:name=".Teams.Team10" />
        <activity android:name=".FixtureActivity" />
        <activity android:name=".Tml"></activity>
    </application>

</manifest>